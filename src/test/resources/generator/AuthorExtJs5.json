Ext.define("MyApp.Author",
{
  extend : "Ext.data.Model",
  uses : [ "MyApp.Book" ],
  requires : [ "Ext.data.proxy.Direct", "Ext.data.identifier.Sequential" ],
  identifier : "sequential",
  clientIdProperty : "theClientId",
  fields : [ {
    name : "id",
    type : "string"
  }, {
    name : "theClientId",
    type : "string"
  }, {
    name : "title",
    type : "string",
    defaultValue : "Mr."
  }, {
    name : "firstName",
    type : "string",
    defaultValue : undefined
  }, {
    name : "lastName",
    type : "string",
    convert : null
  }, {
    name : "book_id",
    type : "int"
  } ],
  associations : [ {
    type : "belongsTo",
    model : "MyApp.Book",
    associationKey : "book",
    foreignKey : "book_id",
    primaryKey : "isbn",
    setterName : "setBook",
    getterName : "getBook"
  } ],
  proxy : {
    type : "direct",
    writer : {
      writeAllFields : true,
      clientIdProperty : "theClientId"
    }
  }
});